{
  "name": "console-log-pipe-test-app",
  "version": "1.0.0",
  "description": "Test application for Console Log Pipe packages",
  "type": "module",
  "scripts": {
    "install-packages": "npm install @kansnpms/console-log-pipe-cli@latest @kansnpms/console-log-pipe-client@latest",
    "test": "node test-integration.js",
    "test:browser": "node test-browser.js",
    "test:cli": "node test-cli.js",
    "test:all": "npm run test:cli && npm run test:browser && npm run test",
    "start:server": "clp start --port 3001",
    "dev": "concurrently \"npm run start:server\" \"npm run test:browser\"",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "@kansnpms/console-log-pipe-cli": "^1.1.24",
    "@kansnpms/console-log-pipe-client": "^1.1.24"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "express": "^4.18.2",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "Console Log Pipe Test",
  "license": "MIT"
}
